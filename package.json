{
  "name": "react-stylish",
  "version": "0.0.1",
  "private": true,
  "description": "A simple utility for styling components based on their state and props.",
  "main": "dom/index.js",
  "scripts": {
    "lint": "node_modules/.bin/eslint .",
    "test:unit": "node_modules/.bin/karma start",
    "test": "npm run lint && npm run test:unit",
    "compile": "mkdir -p dist && BABEL_ENV=production node_modules/.bin/babel src --out-dir dist",
    "build": "npm run compile && git add -A . && git commit -a -m 'updated dist versions'",
    "release:patch": "npm run compile && npm version patch && git push --follow-tags && npm publish",
    "release:minor": "npm run compile && npm version minor && git push --follow-tags && npm publish",
    "release:major": "npm run compile && npm version major && git push --follow-tags && npm publish"
  },
  "keywords": [
    "styles",
    "react-native"
  ],
  "author": "Chris Sauve (chris.sauve@shopify.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shopify/react-native-stylish/issues"
  },
  "homepage": "https://github.com/Shopify/react-native-stylish#readme",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.24",
    "babel-eslint": "^4.1.1",
    "babel-loader": "^5.3.2",
    "eslint": "^1.5.0",
    "eslint-config-shopify": "^5.6.0",
    "eslint-plugin-react": "^3.6.0",
    "eslint-plugin-shopify": "^2.0.0",
    "expect": "^1.11.1",
    "inject-loader": "^2.0.1",
    "isparta-loader": "^1.0.0",
    "istanbul": "^0.3.21",
    "istanbul-instrumenter-loader": "^0.1.3",
    "karma": "^0.13.10",
    "karma-chrome-launcher": "^0.2.0",
    "karma-cli": "^0.1.1",
    "karma-coverage": "^0.5.2",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-phantomjs-shim": "^1.1.1",
    "karma-sinon-chai": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "lodash": "^3.10.1",
    "mocha": "^2.3.3",
    "mockery": "^1.4.0",
    "phantomjs": "^1.9.18",
    "proxyquire": "^1.7.2",
    "react": "^0.13.3",
    "react-native": "^0.11.4",
    "rewire": "^2.3.4",
    "rewire-webpack": "^1.0.0",
    "rimraf": "^2.4.3",
    "sinon": "^1.17.1",
    "webpack": "^1.12.2"
  },
  "dependencies": {
    "lodash.compact": "^3.0.0",
    "lodash.flatten": "^3.0.2",
    "lodash.isarray": "^3.0.4",
    "lodash.isempty": "^3.0.4",
    "lodash.isfunction": "^2.4.1",
    "lodash.isnumber": "^3.0.1",
    "lodash.isobject": "^3.0.2",
    "lodash.merge": "^3.3.2"
  }
}
