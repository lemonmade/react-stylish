{
  "name": "react-stylish",
  "version": "0.0.1",
  "private": true,
  "description": "A simple utility for styling components based on their state and props.",
  "main": "src/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "clean": "$(npm bin)/rimraf lib dist coverage",
    "lint": "$(npm bin)/eslint .",
    "test": "$(npm bin)/mocha test/ --recursive --compilers js:babel/register --reporter spec",
    "test:cover": "$(npm bin)/istanbul cover --dir coverage -- $(npm bin)/_mocha test/ --recursive --compilers js:babel/register --reporter spec",
    "test:watch": "npm test -- --watch --reporter min",
    "check": "npm run lint && npm run test",
    "build:lib": "$(npm bin)/babel src --out-dir lib",
    "build:umd": "$(npm bin)/webpack src/index.js dist/stylish.js --config webpack.config.development.js",
    "build:umd:min": "$(npm bin)/webpack src/index.js dist/stylish.min.js --config webpack.config.production.js",
    "build": "npm run clean && npm run build:lib && npm run build:umd && npm run build:umd:min",
    "preversion": "npm run clean && npm run check",
    "version": "npm run build",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublish": "npm run clean && npm run build"
  },
  "keywords": [
    "styles",
    "react",
    "react-native"
  ],
  "author": "Chris Sauve (chris.sauve@shopify.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shopify/react-native-stylish/issues"
  },
  "homepage": "https://github.com/Shopify/react-native-stylish#readme",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.24",
    "babel-eslint": "^4.1.1",
    "babel-loader": "^5.3.2",
    "chai": "^3.4.0",
    "coveralls": "^2.11.4",
    "eslint": "^1.5.0",
    "eslint-config-shopify": "^5.6.0",
    "eslint-plugin-react": "^3.6.0",
    "eslint-plugin-shopify": "^2.0.0",
    "expect": "^1.11.1",
    "istanbul": "^0.3.21",
    "jsdom": "^5.4.0",
    "lodash": "^3.10.1",
    "mocha": "^2.3.3",
    "proxyquire": "^1.7.3",
    "react": "^0.14.1",
    "react-native": "^0.13.0",
    "rimraf": "^2.4.3",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.12.2"
  },
  "dependencies": {
    "exenv": "^1.2.0",
    "lodash.compact": "^3.0.0",
    "lodash.flatten": "^3.0.2",
    "lodash.isarray": "^3.0.4",
    "lodash.isempty": "^3.0.4",
    "lodash.isfunction": "^2.4.1",
    "lodash.isnumber": "^3.0.1",
    "lodash.isobject": "^3.0.2",
    "lodash.merge": "^3.3.2"
  }
}
